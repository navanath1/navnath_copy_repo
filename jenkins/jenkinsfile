pipeline{
    agent any
    stages{
        stage('build'){
            steps{
                sh 'echo "build is successful on build stage"' 
            }
        }
stage('test'){
            steps{
                sh 'echo "build is successful test stage"' 
            }
        }
stage('deploy'){
            steps{
                sh 'echo "build is successful deploy stage"' 
            }
        }
    }
}
